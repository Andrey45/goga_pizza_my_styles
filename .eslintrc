{
  "extends": [
    "plugin:vue/strongly-recommended",
    "eslint:recommended"
  ],
  "plugins": [
    "eslint-plugin-vue",
    "html"
  ],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "indent": [
      2,
      "tab"
    ],
    "no-tabs": 0
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2017
  }
}
